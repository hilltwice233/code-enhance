{
  "name": "codeorama",
  "description": "A vscode toolkit that make coding more comfortable.",
  "version": "0.0.0",
  "type": "commonjs",
  "private": true,
  "engines": {
    "vscode": "^1.82.0"
  },
  "main": "./source/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "Code Orama Dark Source",
        "id": "codeorama.dark.source",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-color-theme.json"
      },
      {
        "label": "Code Orama Light Source",
        "id": "codeorama.light.source",
        "uiTheme": "vs",
        "path": "./themes/light-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "hello.world",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "prettier": "prettier . -w"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/vscode": "^1.83.1",
    "@vscode/vsce": "^2.22.0",
    "prettier": "^3.0.3"
  }
}
