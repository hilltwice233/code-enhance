{
  "colors": {
    "activityBar.background": "#181717",
    "activityBar.activeBorder": "#359edb",
    "activityBar.border": "#2f3133",

    "commandCenter.background": "#181717",
    "commandCenter.border": "#2f3133",

    "editor.background": "#212020",
    "editor.foreground": "#aca9a5",
    "editor.lineHighlightBorder": "#00000000",
    "editor.lineHighlightBackground": "#181717",
    "editor.wordHighlightBackground": "#1e2830",
    "editorBracketMatch.border": "#00000000",
    "editorBracketMatch.background": "#76848b",

    "editorLineNumber.foreground": "#6b6965",
    "editorLineNumber.activeForeground": "#55b6ed",

    "panel.background": "#181717",
    "panel.border": "#2f3133",
    "panelTitle.activeBorder": "#359edb",

    "sideBar.background": "#181717",
    "sideBar.border": "#2f3133",

    "statusBar.background": "#181717",
    "statusBar.border": "#2f3133",
    "statusBarItem.remoteBackground": "#167cb7",

    "tab.activeBorderTop": "#359edb",
    "tab.activeBackground": "#212020",
    "tab.inactiveBackground": "#181717",
    "tab.border": "#2f3133",

    "titleBar.activeBackground": "#181717",
    "titleBar.inactiveBackground": "#232121",
    "titleBar.border": "#2f3133"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "method": {"fontStyle": "italic"},
    "memberOperatorOverload": {"foreground": "#646769", "fontStyle": ""},
    "comment.documentation": {"foreground": "#548759"},
    "parameter.label": {"foreground": "#628a91", "fontStyle": "italic"},
    "decorator": {"foreground": "#6fcad8", "fontStyle": "italic"},
    "annotation": {"foreground": "#6fcad8", "fontStyle": "italic"},
    "class.annotation": {"foreground": "#6fcad8", "fontStyle": "italic"},
    "property.annotation": {"foreground": "#6fcad8", "fontStyle": "italic"},
    "property.static": {"foreground": "#bd93d5", "fontStyle": ""},
    "enumMember": {"foreground": "#bd93d5", "fontStyle": "italic"},
    "variable.defaultLibrary": {"foreground": "#bd93d5", "fontStyle": "bold"}
  },
  "tokenColors": [
    {
      "scope": ["source.ignore"],
      "settings": {
        "foreground": "#c8c6c2",
        "fontStyle": ""
      }
    },
    {
      "scope": ["text.html.markdown", "zh-mark"],
      "settings": {
        "foreground": "#aba9a5",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["comment", "zh-mark"],
      "settings": {
        "foreground": "#646769",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "fenced_code.block.language.markdown",
        "markup.underline.link",
        "markup.strikethrough"
      ],
      "settings": {
        "foreground": "#646769",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.brace",
        "punctuation",
        "storage.modifier.array.bracket.square",
        "source.dart",
        "meta.body.function.definition"
      ],
      "settings": {
        "foreground": "#5c6063",
        "fontStyle": ""
      }
    },
    {
      "scope": ["storage.type", "keyword"],
      "settings": {
        "foreground": "#d16d6d",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.inline.raw.string", "zh-mark"],
      "settings": {
        "foreground": "#d16d6d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.operator.new",
        "keyword.other.using",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#d16d6d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.language.json"],
      "settings": {
        "foreground": "#d16d6d",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["support.type", "entity.name.type"],
      "settings": {
        "foreground": "#d2b997",
        "fontStyle": ""
      }
    },
    {
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#d2b997",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.type.enum"],
      "settings": {
        "foreground": "#d2b997",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#d2b997",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#d2b997",
        "fontStyle": "italic underline"
      }
    },
    {
      "scope": ["entity.name.function", "markup.bold"],
      "settings": {
        "foreground": "#fbd640"
      }
    },

    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#fbd640",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#c2c0b3",
        "fontStyle": ""
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#c2c0b3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#c2c0b3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "markup.quote.markdown"],
      "settings": {
        "foreground": "#7ba74a"
      }
    },
    {
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#548759",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "constant.character.escape",
        "entity.name.function.operator",
        "storage.modifier.pointer"
      ],
      "settings": {
        "foreground": "#6fcad8",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "storage.type.class.jsdoc",
        "punctuation.definition.block.tag.jsdoc"
      ],
      "settings": {
        "foreground": "#6fcad8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#94b6c6",
        "fontStyle": ""
      }
    },
    {
      "scope": ["support.type.property-name.table.toml"],
      "settings": {
        "foreground": "#34ade6",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "constant.numeric",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#34ade6",
        "fontStyle": "bold"
      }
    },

    {
      "scope": ["string.other.link.title", "zh-mark"],
      "settings": {
        "foreground": "#34ade6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other.enummember",
        "constant.other.placeholder",
        "constant.other.option"
      ],
      "settings": {
        "foreground": "#bd93d5",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.heading.markdown", "entity.name.function.preprocessor"],
      "settings": {
        "foreground": "#bd93d5",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.type.module",
        "support.type.property-name",
        "entity.name.tag",
        "keyword.other.definition.ini",
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#bd93d5",
        "fontStyle": "italic bold"
      }
    }
  ]
}
