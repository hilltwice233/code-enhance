{
  "colors": {
    "activityBar.background": "#181717",
    "activityBar.activeBorder": "#359edb",
    "activityBar.border": "#2f3133",

    "commandCenter.background": "#181717",
    "commandCenter.border": "#2f3133",

    "editor.background": "#212020",
    "editor.foreground": "#aca9a5",

    "sideBar.background": "#181717",
    "sideBar.border": "#2f3133",

    "statusBar.background": "#181717",
    "statusBar.border": "#2f3133",
    "statusBarItem.remoteBackground": "#167cb7",

    "tab.activeBorderTop": "#359edb",
    "tab.activeBackground": "#212020",
    "tab.inactiveBackground": "#181717",
    "tab.border": "#2f3133",

    "titleBar.activeBackground": "#181717",
    "titleBar.inactiveBackground": "#232121",
    "titleBar.border": "#2f3133"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "method": {"fontStyle": "italic"},
    "variable.defaultLibrary": {"foreground": "#bd93d5", "fontStyle": "bold"}
  },
  "tokenColors": [
    {
      "scope": ["source.ignore"],
      "settings": {
        "foreground": "#c8c6c2",
        "fontStyle": ""
      }
    },
    {
      "scope": ["comment", "zh-mark"],
      "settings": {
        "foreground": "#646769",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.brace", "punctuation"],
      "settings": {
        "foreground": "#5c6063",
        "fontStyle": ""
      }
    },
    {
      "scope": ["storage.type"],
      "settings": {
        "foreground": "#d16d6d",
        "fontStyle": ""
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#d16d6d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.type", "entity.name.type"],
      "settings": {
        "foreground": "#e8a043",
        "fontStyle": ""
      }
    },
    {
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#e8a043",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.type.enum"],
      "settings": {
        "foreground": "#e8a043",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#f7c42a"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#c2c0b3",
        "fontStyle": ""
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#c2c0b3",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#c2c0b3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#7ba74a"
      }
    },
    {
      "scope": ["comment.block.documentation"],
      "settings": {
        "foreground": "#548759",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword.operator", "constant.character.escape"],
      "settings": {
        "foreground": "#6fcad8",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "storage.type.class.jsdoc",
        "punctuation.definition.block.tag.jsdoc"
      ],
      "settings": {
        "foreground": "#6fcad8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#34ade6",
        "fontStyle": ""
      }
    },
    {
      "scope": ["variable.other.enummember"],
      "settings": {
        "foreground": "#bd93d5",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.type.module"],
      "settings": {
        "foreground": "#bd93d5",
        "fontStyle": "italic bold"
      }
    }
  ]
}
