{
  "colors": {
    "activityBar.activeBorder": "#3a80e1",
    "activityBar.background": "#fefdfa",
    "activityBar.border": "#e4e3e1",
    "activityBar.foreground": "#383d43",
    "activityBar.inactiveForeground": "#7d868f",

    "commandCenter.background": "#ffffff",
    "commandCenter.border": "#e4e3e1",

    "editor.background": "#ffffff",
    "editor.foreground": "#3e4449",
    "editor.lineHighlightBorder": "#00000000",
    "editor.lineHighlightBackground": "#fcfaf7",
    "editor.wordHighlightBackground": "#eaf3e1",
    "editorBracketMatch.border": "#00000000",
    "editorBracketMatch.background": "#e3f0d6",
    "editor.selectionBackground": "#cce2ef",

    "input.border": "#e4e3e1",
    "input.background": "#ffffff",

    "sideBar.background": "#fefdfa",
    "sideBar.border": "#e4e3e1",

    "statusBar.background": "#fefdfa",
    "statusBar.foreground": "#383d43",
    "statusBar.border": "#e4e3e1",
    "statusBarItem.remoteBackground": "#2663b8",
    "statusBarItem.errorForeground": "#c83939",

    "tab.activeBackground": "#ffffff",
    "tab.inactiveBackground": "#fefdfa",
    "tab.border": "#e4e3e1",
    "tab.activeBorderTop": "#3a80e1",

    "titleBar.activeBackground": "#fefdfa",
    "titleBar.inactiveBackground": "#f5f4ef",
    "titleBar.border": "#e4e3e1"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {},
  "tokenColors": [
    {
      "scope": ["source.ignore", "meta.structure.dictionary.value", "variable"],
      "settings": {
        "foreground": "#414243",
        "fontStyle": ""
      }
    },
    {
      "scope": ["text.html.markdown", "zh-mark"],
      "settings": {
        "foreground": "#414243",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["comment", "zh-mark"],
      "settings": {
        "foreground": "#9fa4a6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link", "markup.strikethrough"],
      "settings": {
        "foreground": "#9fa4a6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#b4bcc0",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "constant.character.escape",
        "keyword",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#a83333",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "storage.type.built-in",
        "variable.parameter.positional.shell",
        "variable.language.special",
        "markup.bold",
        "constant.language"
      ],
      "settings": {
        "foreground": "#a83333",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.control.directive.include",
        "punctuation.definition.directive"
      ],
      "settings": {
        "foreground": "#a83333",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["markup.inline.raw.string", "zh-mark"],
      "settings": {
        "foreground": "#a83333",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#987c2f",
        "fontStyle": "italic underline"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#83a45c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.quote", "zh-mark"],
      "settings": {
        "foreground": "#83a45c",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#1596c4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#0e71a6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.type.property-name", "entity.name.tag"],
      "settings": {
        "foreground": "#0e71a6",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["entity.name.function", "punctuation.definition.list.begin"],
      "settings": {
        "foreground": "#0e71a6",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.namespace",
        "constant.other.option",
        "string.other.link.title"
      ],
      "settings": {
        "foreground": "#af85cf",
        "fontStyle": "bold"
      }
    }
  ]
}
