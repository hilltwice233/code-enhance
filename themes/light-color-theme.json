{
  "colors": {
    "activityBar.activeBorder": "#3a80e1",
    "activityBar.background": "#fefdfa",
    "activityBar.border": "#e4e3e1",
    "activityBar.foreground": "#383d43",
    "activityBar.inactiveForeground": "#7d868f",

    "commandCenter.background": "#ffffff",
    "commandCenter.border": "#e4e3e1",

    "editor.background": "#ffffff",
    "editor.foreground": "#3e4449",
    "editor.lineHighlightBorder": "#00000000",
    "editor.lineHighlightBackground": "#fcfaf7",
    "editor.wordHighlightBackground": "#eaf3e1",
    "editorBracketMatch.border": "#00000000",
    "editorBracketMatch.background": "#e3f0d6",
    "editor.selectionBackground": "#cce2ef",

    "editorLineNumber.foreground": "#9fa4a6",
    "editorLineNumber.activeForeground": "#3a80e1",

    "input.border": "#e4e3e1",
    "input.background": "#ffffff",

    "markdown.extension.editor.codeSpan.border": "#00000000",
    "markdown.extension.editor.codeSpan.background": "#e4e3e1",

    "panel.background": "#fefdfa",
    "panel.border": "#e4e3e1",
    "panelTitle.activeBorder": "#3a80e1",

    "sideBar.background": "#fefdfa",
    "sideBar.border": "#e4e3e1",

    "statusBar.background": "#fefdfa",
    "statusBar.foreground": "#383d43",
    "statusBar.border": "#e4e3e1",
    "statusBarItem.remoteBackground": "#2663b8",
    "statusBarItem.errorForeground": "#c83939",

    "tab.activeBackground": "#ffffff",
    "tab.inactiveBackground": "#fefdfa",
    "tab.border": "#e4e3e1",
    "tab.activeBorderTop": "#3a80e1",

    "titleBar.activeBackground": "#fefdfa",
    "titleBar.inactiveBackground": "#f5f4ef",
    "titleBar.border": "#e4e3e1"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enum": {"foreground": "#5d6266", "fontStyle": "italic bold"},
    "type": {"foreground": "#5d6266", "fontStyle": "bold"},
    "interface": {"foreground": "#5d6266", "fontStyle": "italic bold"},
    "parameter.label": {"foreground": "#9fa4a6", "fontStyle": "italic"},
    "comment.documentation": {"foreground": "#83a45c"},
    "method": {"fontStyle": "italic"},
    "annotation": {"foreground": "#af85cf", "fontStyle": "italic"},
    "property.annotation": {"foreground": "#af85cf", "fontStyle": "italic"}
  },
  "tokenColors": [
    {
      "scope": ["support.class"],
      "settings": {
        "foreground": "#5d6266",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#5d6266",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#414243",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["source.ignore", "meta.structure.dictionary.value", "variable"],
      "settings": {
        "foreground": "#414243",
        "fontStyle": ""
      }
    },
    {
      "scope": ["text.html.markdown", "zh-mark"],
      "settings": {
        "foreground": "#414243",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["comment", "zh-mark"],
      "settings": {
        "foreground": "#9fa4a6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link", "markup.strikethrough"],
      "settings": {
        "foreground": "#9fa4a6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "punctuation",
        "storage.modifier.array.bracket",
        "source.dart",
        "storage.type.function.arrow",
        "keyword.operator",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#b4bcc0",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "constant.character.escape",
        "keyword",
        "storage.type",
        "keyword.operator.expression.typeof"
      ],
      "settings": {
        "foreground": "#a83333",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "variable.parameter.positional.shell",
        "variable.language.special",
        "markup.bold",
        "constant.language",
        "keyword.control",
        "keyword.operator.new"
      ],
      "settings": {
        "foreground": "#a83333",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "keyword.control.directive.include",
        "punctuation.definition.directive",
        "keyword.control.directive.define"
      ],
      "settings": {
        "foreground": "#a83333",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": [
        "storage.modifier",
        "variable.language.this",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#a83333",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw.string", "zh-mark"],
      "settings": {
        "foreground": "#a83333",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#987c2f",
        "fontStyle": "italic underline"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#83a45c",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.quote", "comment.block.documentation", "zh-mark"],
      "settings": {
        "foreground": "#83a45c",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#1596c4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#0e71a6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "support.type.property-name",
        "entity.name.tag",
        "keyword.other.definition.ini"
      ],
      "settings": {
        "foreground": "#0e71a6",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["entity.name.function", "punctuation.definition.list.begin"],
      "settings": {
        "foreground": "#0e71a6",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "storage.type.annotation",
        "storage.type.class.jsdoc",
        "punctuation.definition.block.tag"
      ],
      "settings": {
        "foreground": "#af85cf",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant.other.placeholder"],
      "settings": {
        "foreground": "#af85cf",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.namespace",
        "constant.other.option",
        "string.other.link.title"
      ],
      "settings": {
        "foreground": "#af85cf",
        "fontStyle": "bold"
      }
    }
  ]
}
