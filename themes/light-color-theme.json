{
  "colors": {
    "activityBar.activeBorder": "#3a80e1",
    "activityBar.background": "#fefdfa",
    "activityBar.border": "#e4e3e1",
    "activityBar.foreground": "#383d43",
    "activityBar.inactiveForeground": "#7d868f",

    "commandCenter.background": "#ffffff",
    "commandCenter.border": "#e4e3e1",

    "editor.background": "#ffffff",
    "editor.foreground": "#3e4449",
    "editor.lineHighlightBorder": "#00000000",
    "editor.lineHighlightBackground": "#fcfaf7",
    "editor.wordHighlightBackground": "#eaf3e1",
    "editorBracketMatch.border": "#00000000",
    "editorBracketMatch.background": "#e3f0d6",

    "input.border": "#e4e3e1",
    "input.background": "#ffffff",

    "sideBar.background": "#fefdfa",
    "sideBar.border": "#e4e3e1",

    "statusBar.background": "#fefdfa",
    "statusBar.foreground": "#383d43",
    "statusBar.border": "#e4e3e1",
    "statusBarItem.remoteBackground": "#2663b8",
    "statusBarItem.errorForeground": "#c83939",

    "tab.activeBackground": "#ffffff",
    "tab.inactiveBackground": "#fefdfa",
    "tab.border": "#e4e3e1",
    "tab.activeBorderTop": "#3a80e1",

    "titleBar.activeBackground": "#fefdfa",
    "titleBar.inactiveBackground": "#f5f4ef",
    "titleBar.border": "#e4e3e1"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {},
  "tokenColors": [
    {
      "scope": ["source.ignore"],
      "settings": {
        "foreground": "#414243",
        "fontStyle": ""
      }
    },
    {
      "scope": ["comment", "zh-mark"],
      "settings": {
        "foreground": "#adafb0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#c3c8ca",
        "fontStyle": ""
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#a83333"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#a83333",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#1596c4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.type.property-name", "entity.name.tag"],
      "settings": {
        "foreground": "#0e71a6",
        "fontStyle": "italic bold"
      }
    }
  ]
}
